#print all the prime number between x and y using function and get their sum 
def prime_check(x,y):
    count=0
    s=0
    while x<=y: 
        for i in range(1,x+1):
            if x%i==0:
                count+=1
        if count==2:
            print(i)
            s=s+i
        
        x=x+1
    return s 
    
    
# main code 

prime_check(1,5)
